<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Categorias">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glh2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glh3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.72" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glh4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.84" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glh5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.96" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.01" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.99" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="258dp"
        android:layout_height="157dp"
        android:src="@drawable/logotipo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/glv3"
        app:layout_constraintStart_toStartOf="@+id/glv1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center|top"
        android:paddingTop="37dp"
        android:text="@string/devtinary"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/glh2"
        app:layout_constraintEnd_toStartOf="@+id/glv3"
        app:layout_constraintStart_toStartOf="@+id/glv1"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/ibmsql"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/botonsql"
        app:layout_constraintBottom_toTopOf="@+id/glh3"
        app:layout_constraintEnd_toStartOf="@+id/glv2"
        app:layout_constraintStart_toStartOf="@+id/glv1"
        app:layout_constraintTop_toTopOf="@+id/glh2" />

    <ImageView
        android:id="@+id/ibkotlin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/botonkotlin"
        app:layout_constraintBottom_toTopOf="@+id/glh3"
        app:layout_constraintEnd_toStartOf="@+id/glv3"
        app:layout_constraintStart_toStartOf="@+id/glv2"
        app:layout_constraintTop_toTopOf="@+id/glh2" />

    <ImageView
        android:id="@+id/ibhtml"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/botonhtml"
        app:layout_constraintBottom_toTopOf="@+id/glh4"
        app:layout_constraintEnd_toStartOf="@+id/glv2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/glv1"
        app:layout_constraintTop_toTopOf="@+id/glh3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/ibnode"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/botonnode"
        app:layout_constraintBottom_toTopOf="@+id/glh4"
        app:layout_constraintEnd_toStartOf="@+id/glv3"
        app:layout_constraintStart_toStartOf="@+id/glv2"
        app:layout_constraintTop_toTopOf="@+id/glh3" />

    <ImageView
        android:id="@+id/ibionic"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="0dp"
        android:src="@drawable/botonionic"
        app:layout_constraintBottom_toTopOf="@+id/glh5"
        app:layout_constraintEnd_toStartOf="@+id/glv2"
        app:layout_constraintStart_toStartOf="@+id/glv1"
        app:layout_constraintTop_toTopOf="@+id/glh4" />

    <ImageView
        android:id="@+id/ibjs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/botonjs"
        app:layout_constraintBottom_toTopOf="@+id/glh5"
        app:layout_constraintEnd_toStartOf="@+id/glv3"
        app:layout_constraintStart_toStartOf="@+id/glv2"
        app:layout_constraintTop_toTopOf="@+id/glh4" />

</androidx.constraintlayout.widget.ConstraintLayout>